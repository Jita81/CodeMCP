# Cursor Background Agent MCP Server Configuration
# Copy this file to .env and fill in your actual values

# =======================
# REQUIRED CONFIGURATION
# =======================

# Your Cursor API key from https://cursor.com/integrations
CURSOR_API_KEY=your_cursor_api_key_here

# =======================
# OPTIONAL CONFIGURATION
# =======================

# Cursor API base URL (use default if not specified)
CURSOR_API_BASE_URL=https://api.cursor.com

# Logging configuration
LOG_LEVEL=INFO

# MCP Server configuration
MCP_SERVER_NAME=cursor-background-agent-mcp
MCP_SERVER_VERSION=1.0.0

# Web server configuration
WEB_SERVER_HOST=0.0.0.0
WEB_SERVER_PORT=8080
